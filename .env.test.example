# Testing Environment Configuration
ENV=testing
LOG_LEVEL=debug
WORKERS=1
CALCULATION_THREAD_POOL_SIZE=1
API_V1_STR=/api/v1
SECRET_KEY=test_secret_key

# Supabase Configuration (Mock Values)
NEXT_PUBLIC_SUPABASE_URL=https://example.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=dummy_anon_key
SUPABASE_JWT_SECRET=dummy_jwt_secret
SUPABASE_SERVICE_ROLE_KEY=dummy_service_role_key
SUPABASE_URL=https://example.supabase.co
SUPABASE_KEY=dummy_key

# Upstash Redis Configuration (Mock Values)
UPSTASH_REDIS_HOST=localhost
UPSTASH_REDIS_PORT=6379
UPSTASH_REDIS_PASSWORD=dummy_password

# CORS Settings (Must be in JSON format)
CORS_ORIGINS=["http://localhost:3000", "https://example.com"]

# Celery Task Queue Settings
CELERY_TASK_ALWAYS_EAGER=true
CELERY_CONCURRENCY=1

# Cache and Database Settings
CACHE_TTL=60
BATCH_SIZE=10
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# Monitoring and Logging
LOGGING_JSON_FORMAT=false
PROMETHEUS_ENABLED=false
